<%= simple_form_for @real_state_agency do |f| %>
  <%= f.file_field :avatar %>
  <%= f.input :trade_name %>
  <%= f.input :social_name %>
  <%= f.input :cnpj %>
  <%= f.input :email %>
  <%= f.input :mobile_number %>
  <%= f.input :phone_number %>
  <%= f.input :fax %>
  <%= f.input :email %>

  <%= f.simple_fields_for :address, @real_state_agency.address do |b| %>
    <%= render 'addresses/form', f: b %>
  <% end %>

  <%= f.input :active, as: :radio_buttons %>

  <%= f.button :submit %>
<% end %>
