<%= simple_form_for @real_state_agency, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.file_field :avatar %>
  <%= f.input :trade_name %>
  <%= f.input :social_name %>
  <%= f.input :cnpj %>
  <%= f.input :email %>
  <%= f.input :mobile_number %>
  <%= f.input :phone_number %>
  <%= f.input :fax %>
  <%= f.input :email %>

  <%= f.simple_fields_for :address, @real_state_agency.address do |b| %>
    <%= render 'addresses/form', f: b %>
  <% end %>

    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                real_state_agencies_path, :class => 'btn btn-default' %>
<% end %>
